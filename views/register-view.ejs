<script type="text/javascript">
function Validate()
{
        var x = document.sform.phone.value;
        var y= document.sform.password.value;
         var z= document.sform.cpassword.value;

        if(isNaN(x)|| x.indexOf(" ")!=-1){
              alert("Enter numeric value");return false; }
        if (x.length < 9 || x.length > 10 ){
                alert("enter 10 digits"); return false;
           }
        if (x.charAt(0)!="9"){
                alert("it should start with 9");
                return false;
           }
        if(y!=z){
          alert("Please enter same password");
          return false;
        }   
}
</script>
